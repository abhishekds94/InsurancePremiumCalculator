                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/tax_val"
                        android:textAlignment="center"
                        android:textSize="18dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/per_symbol"
                        android:textAlignment="center"
                        android:textSize="18dp" />

                </TableRow>


                <View
                    android:layout_width="fill_parent"
                    android:layout_height="2dp"
                    android:background="#2E8B57"
                    android:layout_marginTop="5dp"/>


                <TableRow
                    android:padding="5dp">

                    <TextView
                        android:id="@+id/ppdisplay_bus_above60_total"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textAlignment="center"
                        android:text="@string/total_premium"
                        android:textStyle="bold"
                        android:textColor="#3498DB"
                        android:textSize="25dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/colon_symb"
                        android:textAlignment="textStart"
                        android:textColor="#3498DB"
                        android:textSize="22dp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/ppdisplay_bus_above60_total_value"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text=""
                        android:textAlignment="center"
                        android:textColor="#3498DB"
                        android:textSize="22dp"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/rupee_symbol"
                        android:textColor="#3498DB"
                        android:textStyle="bold"
                        android:textAlignment="center"
                        android:textSize="22dp" />

                </TableRow>

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="2dp"
                    android:background="#2E8B57"
                    android:layout_marginTop="5dp"/>


                <LinearLayout
                    xmlns:android="http://schemas.android.com/apk/res/android"
                    xmlns:app="http://schemas.android.com/apk/res-auto"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:paddingTop="30dp">

                    <at.markushi.ui.CircleButton
                        android:id="@+id/ppdisplay_bus_above60_home"
                        android:layout_width="0dp"
                        android:layout_height="70dp"
                        android:layout_weight="1"
                        android:src="@drawable/home"
                        android:layout_marginBottom="70dp"
                        app:cb_color="#3498DB"
                        app:cb_pressedRingWidth="10dip"/>

                    <at.markushi.ui.CircleButton
                        android:id="@+id/ppdisplay_bus_above60_share"
                        android:layout_width="0dp"
                        android:layout_height="70dp"
                        android:layout_weight="1"
                        android:src="@drawable/share"
                        android:layout_marginBottom="70dp"
                        app:cb_color="#3498DB"
                        app:cb_pressedRingWidth="10dip"/>

                </LinearLayout>


            </TableLayout>

        </RelativeLayout>
    </ScrollView>
</RelativeLayout>