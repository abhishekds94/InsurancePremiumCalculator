<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:ads="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.avidprogrammers.insurancepremiumcalculator.home_activity"
    android:background="#EAFAF1">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <Button
                android:id="@+id/longterm"
                android:layout_width = "match_parent"
                android:layout_height = "60dp"
                android:layout_weight = "1"
                android:text="longterm Policies"
                android:textAlignment="viewStart"
                android:drawablePadding="20dp"
                android:paddingLeft="20dp"
                android:drawableLeft="@drawable/lt_logo"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="10dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentStart="true"
                android:background="@drawable/border"
                android:onClick="longterm_btn"/>

            <Button
                android:id="@+id/motorcycle"
                android:layout_width = "match_parent"
                android:layout_height = "60dp"
                android:layout_weight = "1"
                android:text="MotorCycle"
                android:textAlignment="viewStart"
                android:drawablePadding="20dp"
                android:paddingLeft="20dp"
                android:drawableLeft="@drawable/motorcycle"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="10dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentStart="true"
                android:background="@drawable/border"
                android:onClick="motorcycle_btn"/>


            <Button
                android:id="@+id/privatecar"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/motorcycle"
                android:layout_marginTop="10dp"
                android:layout_weight="1"
                android:text="Private Car"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="@drawable/border"
                android:textAlignment="viewStart"
                android:drawablePadding="20dp"
                android:paddingLeft="20dp"
                android:drawableLeft="@drawable/car"
                tools:layout_editor_absoluteX="-165dp"
                tools:layout_editor_absoluteY="551dp"
                android:onClick="privatecar_btn" />

<!--            <FrameLayout-->
<!--                android:id="@+id/ad_frame"-->
<!--                android:layout_width="match_parent"-->
<!--                android:layout_height="wrap_content" />-->

            <Button
                android:id="@+id/taxi_upto6"
