<!--                android:layout_marginRight="10dp" />-->

            <Button
                android:id="@+id/goodsauto_public"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_marginTop="10dp"
                android:layout_weight="1"
                android:text="Public Carrier Goods Auto"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="@drawable/border"
                android:textAlignment="viewStart"
                android:drawablePadding="20dp"
                android:paddingLeft="20dp"
                android:drawableLeft="@drawable/lorry"
                tools:layout_editor_absoluteX="-165dp"
                tools:layout_editor_absoluteY="551dp"
                android:layout_below="@+id/auto"
                android:layout_alignParentStart="true"
                android:onClick="goodsauto_public_btn" />

            <Button
                android:id="@+id/goodsauto_private"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_marginTop="10dp"
                android:layout_weight="1"
                android:text="Private Carrier Goods Auto"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="@drawable/border"
                android:textAlignment="viewStart"
                android:drawablePadding="20dp"
                android:paddingLeft="20dp"
                android:drawableLeft="@drawable/lorry"
                tools:layout_editor_absoluteX="-165dp"
                tools:layout_editor_absoluteY="551dp"
                android:layout_below="@+id/goodsautopublic"
                android:layout_alignParentStart="true"
                android:onClick="goodsauto_private_btn" />

            <Button
                android:id="@+id/commercialvehiclepublic"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_marginTop="10dp"
                android:layout_weight="1"
                android:text="Goods Vehicle - Public"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="@drawable/border"
                android:textAlignment="viewStart"
                android:drawablePadding="20dp"
                android:paddingLeft="20dp"
                android:drawableLeft="@drawable/truck"
                tools:layout_editor_absoluteX="-165dp"
                tools:layout_editor_absoluteY="551dp"
                android:layout_below="@+id/goodsautoprivate"
                android:layout_alignParentStart="true"
                android:onClick="commercialvehiclepublic_btn" />

<!--            <include-->
<!--                layout="@layout/native_ad"-->
<!--                android:id="@+id/ad_frame3"-->
<!--                android:layout_height="wrap_content"-->
<!--                android:layout_width="match_parent"-->
<!--                android:layout_marginTop="10dp"-->
<!--                android:layout_marginLeft="10dp"-->
<!--                android:layout_marginRight="10dp" />-->

            <Button
                android:id="@+id/commercialvehicleprivate"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_marginTop="10dp"
                android:layout_weight="1"
                android:text="Goods Vehicle - Private"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="@drawable/border"
                android:textAlignment="viewStart"
                android:drawablePadding="20dp"
                android:paddingLeft="20dp"
                android:drawableLeft="@drawable/truck"
                tools:layout_editor_absoluteX="-165dp"
                tools:layout_editor_absoluteY="551dp"
                android:layout_below="@+id/commercialvehiclepublic"
                android:layout_alignParentStart="true"
                android:onClick="commercialvehicleprivate_btn" />

            <Button
                android:id="@+id/agri"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_marginTop="10dp"
